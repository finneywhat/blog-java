<div class="row">
  <div class="col-md-8 col-md-offset-2">
    #foreach($post in $posts)
      <div class="well">
        <h3><a href="/users/$post.getUserId()/posts/$post.getId()">$post.getTitle()</a></h3>
        <p>
          #foreach($tag in $post.getTags())
            <span class="label label-info">$tag.getName()</span>
          #end
        </p>
      </div>
    #end
  </div>
</div>
